# Production Environment Configuration for Netlify
# This file configures the frontend to connect to the Railway backend

# Backend URL - Points to Railway production deployment
VITE_BACKEND_URL=https://simple-backend-production-de4c.up.railway.app

# Production mode
NODE_ENV=production

# API Configuration
VITE_USE_MOCK_API=false

# Domain configuration
VITE_DOMAIN_URL=https://subnetscout.com

# Features
VITE_ENABLE_DEBUG_LOGS=false
VITE_ENABLE_ERROR_SIMULATION=false

# Telegram bot configuration
VITE_TELEGRAM_BOT_USERNAME=subnet_scout_bot